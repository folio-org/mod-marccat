<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping package="librisuite.hibernate">
	<class name="IndexMainList"  schema="olisuite"  table="INDEX_MAIN_LIST">
		<id name="indexKey">
			<column name="INDEX_KEY" />
			<generator class="assigned"/>
		</id>
		<property name="indexValueCode">
			<column name="INDEX_VLU_CDE"/>
		</property>
		<property name="indexMainName">
			<column name="INDEX_MAIN_NAME"/>
		</property>
		<property name="indexType">
			<column name="INDEX_TYPE"/>
		</property>
		<property name="language">
			<column name="INDEX_LANGUAGE"/>
		</property>
	</class>
</hibernate-mapping>


