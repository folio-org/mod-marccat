<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping package="librisuite.hibernate">
	<class name="CasFiltri"  schema="custom"  table="T_CAS_FLTR">		
		<id name="idFiltro">
			<column name="FLTR_ID" not-null="true"/>
			<generator class="assigned"/>
		</id>	
		<property name="nomeFiltro">
			<column name="DESCR" sql-type="VARCHAR2 (128)"/>
		</property>	
		<property name="tipoFiltro">
			<column name="FLTR_TYP" sql-type="VARCHAR2 (32)"/>
		</property>		
	</class>
</hibernate-mapping>