<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<class name="librisuite.hibernate.Vendor" table="VNDR">

    <id name="vendorNumber" column="VNDR_NBR">
    	<generator class="assigned"/>
    </id>
    <property name="name"> 
		<column name="VNDR_NME"/>
	</property>
    <property name="abbreviatedName"> 
		<column name="VNDR_ABBREV_NME"/>
	</property>
    <property name="typeCode"> 
		<column name="VNDR_TYP"/>
	</property>
    <property name="owningMainLibraryNumber"> 
		<column name="ORG_NBR"/>
	</property>
    <property name="localOrShared"> 
		<column name="LOCAL_SHARED"/>
	</property>
    <property name="claimPolicyNumber"> 
		<column name="CLM_PLCY_NBR"/>
	</property>
    <property name="formTypeCode"> 
		<column name="VNDR_FORM_TYP_CDE"/>
	</property>
    <property name="paymentTypeCode"> 
		<column name="VNDR_PAYMT_TYP_CDE"/>
	</property>
    <property name="acquisitionTypeCode"> 
		<column name="VNDR_AQSTN_TYP_CDE"/>
	</property>
    <property name="currencyCode"> 
		<column name="CURCY_TYP_CDE"/>
	</property>
    <property name="discount"> 
		<column name="VNDR_DISCOUNT"/>
	</property>
    <property name="url"> 
		<column name="VNDR_URL_NME"/>
	</property>
    <property name="lastUpdateDate"> 
		<column name="VNDR_LST_UPDATE"/>
	</property>
    <property name="languageCode"> 
		<column name="VNDR_LANGUAGE"/>
	</property>
    <property name="note"> 
		<column name="VNDR_NTE"/>
	</property>
    <property name="lastUpdateNumber"> 
		<column name="LST_UPD_NBR"/>
	</property>
    <property name="reportMailIndicator"> 
		<column name="REP_MAIL_IDX"/>
	</property>
    <property name="ediSanNumber"> 
		<column name="VNDR_SAN_NBR"/>
	</property>
    <property name="ediHostName"> 
		<column name="VNDR_EDI_HOST_NME"/>
	</property>
    <property name="ediFilePattern"> 
		<column name="VNDR_EDI_FILE_PATTERN"/>
	</property>
    <property name="ediUserString"> 
		<column name="VNDR_EDI_USR_STRNG"/>
	</property>
</class>
</hibernate-mapping>
