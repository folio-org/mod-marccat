<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping package="librisuite.hibernate">
<class name="CLCTN_CST_RULE"  schema="custom"  table="CLCTN_CST_RULE">
	<id
        name="ruleId"
        type="java.lang.Integer"
        column="RULE_ID"
    >
    	<generator class="native">
			<param name="sequence">CUSTOM.T_CLCTN_CST_RULE_SEQ</param>
		</generator>       
    </id>
    <property name="ruleDescription"> 
		<column name="RULE_DESCRIPTION"/>
	</property>
	<property name="level"> 
		<column name="RULE_NTR_LVL"/>
	</property>
	<property name="dataType"> 
		<column name="RULE_DATE_TYPE"/>
	</property>
	<property name="dataPublRange"> 
		<column name="RULE_DATE_PUBB_RANGE"/>
	</property>
	<property name="dataUploadFrom"> 
		<column name="RULE_DATE_UPLD_FROM"/>
	</property>
	<property name="dataUploadTo"> 
		<column name="RULE_DATE_UPLD_TO"/>
	</property>
	<property name="dataEmbRange"> 
		<column name="RULE_DATE_EMB_RANGE"/>
	</property>
	<property name="dataEmbFrom"> 
		<column name="RULE_DATE_EMB_FROM"/>
	</property>
	<property name="dataEmbTo"> 
		<column name="RULE_DATE_EMB_TO"/>
	</property>
	<property name="dataInsert"> 
		<column name="RULE_DATE_INSERT"/>
	</property>
	<property name="dataUpdate"> 
		<column name="RULE_DATE_UPDATE"/>
	</property>
	<property name="dataProcessing"> 
		<column name="RULE_DATE_PROCESSING"/>
	</property>
	<property name="flagProcessing"> 
		<column name="RULE_FLAG_PROCESSING"/>
	</property>
	<property name="customerCode"> 
		<column name="RULE_CSTMR_CDE"/>
	</property>
	<property name="collectionSource"> 
		<column name="RULE_COLL_SRC"/>
	</property>
	<property name="collectionTarget"> 
		<column name="RULE_COLL_TRG"/>
	</property>
</class>
</hibernate-mapping>
