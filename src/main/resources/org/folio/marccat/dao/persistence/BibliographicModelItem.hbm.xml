<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class
		name="org.folio.marccat.dao.persistence.BibliographicModelItem"
    schema="olisuite"
		table="BIBLIOGRAPHIC_MODEL_ITEM">
		<id name="item">
			<column name="ITEM" sql-type="NUMBER (11)" not-null="true"/>
			<generator class="assigned"/>
		</id>
		<many-to-one name="model" column="MODEL" class="org.folio.marccat.dao.persistence.BibliographicModel" not-null="true"/>
		<property name="recordFields" column="XML_FIELDS" />
	</class>
</hibernate-mapping>