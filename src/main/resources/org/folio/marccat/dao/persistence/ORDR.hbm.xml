<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<class
  name="org.folio.marccat.dao.persistence.Order" table="ORDR"
>

    <id name="orderNumber" column="ORDR_NBR">
    	<generator class="assigned"/>
    </id>
    <property name="vendorOrderNumber"> 
    	<column name="VNDR_ORDR_NBR"/>
    </property>
    <property name="orderDate"> 
    	<column name="ORDR_DTE"/>
    </property>
    <property name="receivedDate"> 
    	<column name="RCVD_DTE"/>
    </property>
    <property name="completedDate"> 
    	<column name="CMPLTD_DTE"/>
    </property>
    <property name="sentDate"> 
    	<column name="SNT_DTE"/>
    </property>
    <property name="cancelledDate"> 
    	<column name="CNCLLD_DTE"/>
    </property>
    <property name="userName"> 
    	<column name="USR_NME"/>
    </property>
    <property name="total"> 
    	<column name="TTL"/>
    </property>
    <property name="totalInput"> 
    	<column name="TTL_INPUT"/>
    </property>
    <property name="orderAcquisitionTypeCode"> 
    	<column name="ORDR_AQSTN_TYP_CDE"/>
    </property>
    <property name="orderPaymentTypeCode"> 
    	<column name="ORDR_PAYMT_TYP_CDE"/>
    </property>
    <property name="orderFormTypeCode"> 
    	<column name="ORDR_FORM_TYP_CDE"/>
    </property>
    <property name="currencyTypeCode"> 
    	<column name="CURCY_TYP_CDE"/>
    </property>
    <property name="currencyExchangeRate"> 
    	<column name="CURCY_XCHNG_RTE"/>
    </property>
    <property name="mainLibraryNumber"> 
    	<column name="ORG_NBR"/>
    </property>
    <property name="note"> 
    	<column name="ORDR_NTE"/>
    </property>
    <property name="statusTypeCode"> 
    	<column name="STATUS_TYP_CDE"/>
    </property>
    <property name="vendorOrganisationNumber"> 
    	<column name="VNDR_ORG_NBR"/>
    </property>
    <property name="vendorContactPersonNumber"> 
    	<column name="VNDR_CNTCT_PRSN_NBR"/>
    </property>
    <property name="libraryAccess"> 
    	<column name="LIB_ACC"/>
    </property>
    <property name="publicAccess"> 
    	<column name="PBLC_ACC"/>
    </property>
    <property name="orderCopyOrderQuantity"> 
    	<column name="ORDR_CPY_ORDR_QTY"/>
    </property>
    <property name="orderCopyReceivedQuantity"> 
    	<column name="ORDR_CPY_RCV_QTY"/>
    </property>
    <property name="orderCopyInvoicedQuantity"> 
    	<column name="ORDR_CPY_INVC_QTY"/>
    </property>
    <property name="orderHasSerial"> 
    	<column name="ORDR_HAS_SRL"/>
    </property>
    <property name="fundOrderItemIndicator"> 
    	<column name="FUND_ORDR_ITM_IND"/>
    </property>
    <property name="lastUpdateNumber"> 
    	<column name="LST_UPD_NBR"/>
    </property>
    <property name="cancelledReasonCode"> 
    	<column name="CNCLLD_RSN_CDE"/>
    </property>
</class>
</hibernate-mapping>
