<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<class name="org.folio.cataloging.dao.persistence.SRL_PRED_PAT_DTL" table="SRL_PRED_PAT_DTL">

    <composite-id>
     <key-property name="predictionPatternNumber" column="PRD_PAT_NBR"/>
     <key-property name="sequenceNumber" column="SEQ_NBR"/>
    </composite-id>
    <property name="partType"> 
    	<column name="TYPE" not-null="true"/>
    </property>
    <property name="patternClass"> 
    	<column name="CLASS" not-null="true"/>
    </property>
    <property name="ordinal"> 
    	<column name="ORDINAL"/>
    </property>
    <property name="timeField"> 
    	<column name="TIME_FIELD" />
    </property>
    <property name="stepCount"> 
    	<column name="STEP_COUNT"/>
    </property>
    <property name="month"> 
    	<column name="MONTH"/>
    </property>
    <property name="day"> 
    	<column name="DAY"/>
    </property>
    <property name="monday"> 
    	<column name="MONDAY"/>
    </property>
    <property name="tuesday"> 
    	<column name="TUESDAY"/>
    </property>
    <property name="wednesday"> 
    	<column name="WEDNESDAY"/>
    </property>
    <property name="thursday"> 
    	<column name="THURSDAY"/>
    </property>
    <property name="friday"> 
    	<column name="FRIDAY"/>
    </property>
    <property name="saturday"> 
    	<column name="SATURDAY"/>
    </property>
    <property name="sunday"> 
    	<column name="SUNDAY"/>
    </property>
</class>
</hibernate-mapping>
