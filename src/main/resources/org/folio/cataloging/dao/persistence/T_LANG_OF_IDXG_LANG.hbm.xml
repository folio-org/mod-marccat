<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping package="org.folio.cataloging.dao.persistence">

	<class name="T_LANG_OF_IDXG_LANG" table="T_LANG_OF_IDXG_LANG">
		<id name="languageIndexing">
			<column name="LANG_OF_IDXG" not-null="true"/>
			<generator class="assigned"/>
		</id>
		<property name="language">
			<column name="LANG" not-null="true"/>
		</property>
	</class>
</hibernate-mapping>
