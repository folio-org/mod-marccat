<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping package="org.folio.cataloging.dao.persistence">
<class name="UserTempPass"   schema="olisuite"  table="USR_TMP_PASS">
	<id name="pass" column="PASS_NBR">
	 	<generator class="assigned" />
	</id>
	<property name="userName" column="USR_NME" />
	<property name="appCode" column="APP_CDE" />
	<property name="lastAccess">
		<column name="LAST_ACCSS" sql-type="DATE"  />
	</property>
</class>
</hibernate-mapping>