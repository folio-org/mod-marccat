<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping package="org.folio.cataloging.dao.persistence">
	<class name="CLCTN_MST_CSTMR"  schema="custom"  table="CLCTN_MST_CSTMR">
	<composite-id>
		<key-property name="collectionCode" column="CLCTN_MST_CDE" />
  	</composite-id>		
		<property name="customerCode">
			<column name="CSTMR_CDE" not-null="true"/>
		</property>
	</class>
</hibernate-mapping>
