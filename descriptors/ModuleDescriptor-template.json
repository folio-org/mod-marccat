{
  "id": "@artifactId@-@version@",
  "name": "MARCcat",
  "requires": [
  ],
  "provides": [
    {
      "id": "marccat",
      "version": "1.4",
      "handlers": [
        {
          "methods": ["GET"],
          "pathPattern": "/search",
          "permissionsRequired": ["marccat.records.collection.get"],
          "modulePermissions": []
        },{
          "methods": ["GET"],
          "pathPattern": "/mergedSearch",
          "permissionsRequired": ["marccat.records.collection.get"],
          "modulePermissions": []
        }, {
          "methods": ["GET"],
          "pathPattern": "/searchVertical",
          "permissionsRequired": ["marccat.records.collection.get"],
          "modulePermissions": []
        },
        {
          "methods": ["GET"],
          "pathPattern": "/countSearch",
          "permissionsRequired": ["marccat.records.collection.get"],
          "modulePermissions": []
        },
        {
          "methods": ["GET"],
          "pathPattern": "/document-count-by-id",
          "permissionsRequired": ["marccat.record.count.get"],
          "modulePermissions": []
        },
        {
          "methods": ["GET"],
          "pathPattern": "/browse",
          "permissionsRequired": ["marccat.records.collection.get"],
          "modulePermissions": []
        },
        {
          "methods": ["GET"],
          "pathPattern": "/headings-by-tag",
          "permissionsRequired": ["marccat.records.collection.get"],
          "modulePermissions": []
        },
        {
          "methods": ["GET"],
          "pathPattern": "/filteredTagsList",
          "permissionsRequired": ["marccat.tag.collection.get"],
          "modulePermissions": []
        },
        {
          "methods": ["GET"],
          "pathPattern": "/filteredTag",
          "permissionsRequired": ["marccat.tag.collection.get"],
          "modulePermissions": []
        },
        {
          "methods": ["GET"],
          "pathPattern": "/record-templates",
          "permissionsRequired": ["marccat.records.collection.get"],
          "modulePermissions": []
        },
        {
          "methods": ["POST"],
          "pathPattern": "/bibliographic-record",
          "permissionsRequired": ["marccat.record.insert.post"],
          "modulePermissions": []
        },
        {
          "methods": ["GET"],
          "pathPattern": "/bibliographic-record/from-template",
          "permissionsRequired": ["marccat.records.collection.get"],
          "modulePermissions": []
        },
        {
          "methods": ["GET"],
          "pathPattern": "/bibliographic-record/duplicate",
          "permissionsRequired": ["marccat.records.collection.get"],
          "modulePermissions": []
        },
        {
          "methods": ["POST"],
          "pathPattern": "/bibliographic-record/fixed-field-display-value",
          "permissionsRequired": ["marccat.tag.value.post"],
          "modulePermissions": []
        },
        {
          "methods": ["GET"],
          "pathPattern": "/fixed-fields-code-groups-by-leader",
          "permissionsRequired": ["marccat.records.collection.get"],
          "modulePermissions": []
        },
        {
          "methods": ["GET"],
          "pathPattern": "/fixed-fields-code-groups",
          "permissionsRequired": ["marccat.records.collection.get"],
          "modulePermissions": []
        },
        {
          "methods": ["GET"],
          "pathPattern": "/header-types",
          "permissionsRequired": ["marccat.records.collection.get"],
          "modulePermissions": []
        },
        {
          "methods": ["POST"],
          "pathPattern": "/create-heading",
          "permissionsRequired": ["marccat.tag.heading.post"],
          "modulePermissions": []
        },
        {
          "methods": ["PUT"],
          "pathPattern": "/bibliographic-record/lock",
          "permissionsRequired": ["marccat.record.lock.put"],
          "modulePermissions": []
        },
        {
          "methods": ["DELETE"],
          "pathPattern": "/bibliographic-record/unlock",
          "permissionsRequired": ["marccat.record.unlock.delete"],
          "modulePermissions": []
        }
      ]
    },
    {
      "id": "_jsonSchemas",
      "version": "1.0",
      "interfaceType" : "multiple",
      "handlers" : [
        {
          "methods" : [ "GET" ],
          "pathPattern" : "/_/jsonSchemas"
        }
      ]
    }, {
      "id": "_ramls",
      "version": "1.0",
      "interfaceType" : "multiple",
      "handlers" : [
        {
          "methods" : [ "GET" ],
          "pathPattern" : "/_/ramls"
        }
      ]
    }, {
      "id": "_tenant",
      "version": "1.0",
      "interfaceType": "system",
      "handlers": [
        {
          "methods": [
            "POST"
          ],
          "pathPattern": "/_/tenant"
        },
        {
          "methods": [
            "DELETE"
          ],
          "pathPattern": "/_/tenant"
        }
      ]
    }
  ],
  "permissionSets": [
    {
      "permissionName": "marccat.records.collection.get",
      "displayName": "MARCcat - get marc records collection",
      "description": "Get marc records collection"
    },
    {
      "permissionName": "marccat.record.count.get",
      "displayName": "MARCcat - get count of document by id",
      "description": "Get count of MARC document by id"
    },
    {
      "permissionName": "marccat.tag.collection.get",
      "displayName": "MARCcat - get marc tag collection for auto suggest",
      "description": "Get marc tag collection for auto suggest"
    },
    {
      "permissionName": "marccat.record.unlock.delete",
      "displayName": "MARCcat - unlock record",
      "description": "Unlock MARC record"
    },
    {
      "permissionName": "marccat.record.lock.put",
      "displayName": "MARCcat - lock record",
      "description": ":Lock MARC record"
    },
    {
      "permissionName": "marccat.tag.heading.post",
      "displayName": "MARCcat - create new heading for tag",
      "description": "Create new heading in tag for MARC record"
    },
    {
      "permissionName": "marccat.tag.value.post",
      "displayName": "MARCcat - tag display value",
      "description": "Display value in tag for MARC record"
    },
    {
      "permissionName": "marccat.record.insert.post",
      "displayName": "MARCcat - save/update record",
      "description": "Save or update MARC record"
    }
  ],
  "launchDescriptor": {
    "dockerImage": "@artifactId@-@version@",
    "dockerArgs": {
      "HostConfig": {
        "PortBindings": {
          "8081/tcp": [
            {
              "HostPort": "%p"
            }
          ]
        }
      }
    },
    "dockerPull": false
  }
}
